-- Созадю файл my.cnf
[client]
user=root
password=***********

-- Создаю базу данных "example"
CREATE DATABASE `example`;

-- Размещаю в базе данных  "example" таблицу "users"
drop table `users`;
CREATE TABLE `users` (
  `id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL COMMENT 'Имя пользователя',
PRIMARY KEY (`id`),
UNIQUE KEY `id` (`id`)

-- Создаю базу данных "sample"
CREATE DATABASE `sample`;

-- Создаю дамп базы данных example, 
-- разворачиваю содержимое дампа в новую базу данных sample

C:\Users\sante>mysqldump -u root -p example >example.sql
Enter password: ***********
C:\Users\sante>mysql -u root -p sample < example.sql
Enter password: ***********

-- дамп единственной таблицы help_keyword базы данных mysql
-- в базу данных `Dianova` выполняю 2 способами 
-- 1. Создаю базу данных `Dianova`: 
CREATE DATABASE `Dianova`;
 use Dianova;

-- создаю дамп таблицы help_keyword 
 CREATE TABLE Dianova.help_keyword
    SELECT *
    FROM mysql.help_keyword
    LIMIT 100;
    
   -- 2. 
   C:\Users\sante>mysqldump mysql --where="true limit 100" help_keyword > dianova.sql